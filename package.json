    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^7.0.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-progress": "0.2.1",
    "rollup-plugin-serve": "^0.1.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rxjs-es": "^5.0.0-beta.12",
    "temp": "^0.8.3",
